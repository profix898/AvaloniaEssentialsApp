<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:avaloniaEssentialsApp="clr-namespace:AvaloniaEssentialsApp"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaEssentialsApp.MainView"
             x:DataType="avaloniaEssentialsApp:MainView">
  <StackPanel Margin="15, 10" Spacing="10">
      <TextBlock FontWeight="Bold">Welcome to Avalonia UI!</TextBlock>
      <TextBlock Text="{Binding DeviceInfoString}" />
  </StackPanel>
</UserControl>
